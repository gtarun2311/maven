@Library('sharedlibrary_cicd')_

pipeline
{
    agent any
    stages
    {
        stage('Download_loans')
        {
            steps
            {
                script
                {
                    cicd.gitDownload("maven")
                }
            }
        }
        stage('Build_loans')
        {
            steps
            {
                script
                {
                    cicd.buildArtifact()
                }
            }
        }
        stage('Deployment_loans')
        {
            steps
            {
                script
                {
                    cicd.deployTomcat("DeclarativePipelinewithSharedLibraries","172.31.30.119","test1")
                }
            }
        }
    }
}

